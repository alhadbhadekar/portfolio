ted":{"line":74,"column":0},"source":"projects/evens-or-odds/actions/deck.js","original":{"line":48,"column":33}},{"generated":{"line":74,"column":7},"source":"projects/evens-or-odds/actions/deck.js","original":{"line":48,"column":32}},{"generated":{"line":74,"column":8},"source":"projects/evens-or-odds/actions/deck.js","original":{"line":48,"column":20}},{"generated":{"line":75,"column":0},"source":"projects/evens-or-odds/actions/deck.js","original":{"line":48,"column":20}},{"generated":{"line":75,"column":5},"source":"projects/evens-or-odds/actions/deck.js","original":{"line":34,"column":15}},{"generated":{"line":75,"column":6},"source":"projects/evens-or-odds/actions/deck.js","original":{"line":34,"column":8}},{"generated":{"line":76,"column":0},"source":"projects/evens-or-odds/actions/deck.js","original":{"line":49,"column":1}},{"generated":{"line":76,"column":3},"source":"projects/evens-or-odds/actions/deck.js","original":{"line":33,"column":36}},{"generated":{"line":77,"column":0},"source":"projects/evens-or-odds/actions/deck.js","original":{"line":33,"column":36}},{"generated":{"line":77,"column":1},"source":"projects/evens-or-odds/actions/deck.js","original":{"line":33,"column":7}}],"sources":{"projects/evens-or-odds/actions/deck.js":"import { DECK, DECK_DRAW } from './types';\r\n\r\nconst API_ADDRESS = 'https://deck-of-cards-api-wrapper.appspot.com'\r\n\r\nexport const fetchDeckSuccess = deckJson => {\r\n    const { remaining, deck_id } = deckJson;\r\n\r\n    return { type: DECK.FETCH_SUCCESS, remaining, deck_id };\r\n}\r\n\r\nexport const fetchDeckError = error => {\r\n    return { type: DECK.FETCH_ERROR, message: error.message};\r\n}\r\n\r\nexport const fetchNewDeck = () => dispatch => {\r\n    //To avoid CORS,\r\n        //1. Either use fetch('https://cors-anywhere.herokuapp.com/https://deckofcardsapi.com/api/deck/new/shuffle/') (Not Recommended)\r\n        //2. Recommended Solution:\r\n        //Use your own home-grown server for dedicated service\r\n\r\n       //Make a fetch request to API\r\n       return fetch(`${API_ADDRESS}/deck/new/shuffle`)\r\n            .then(response => {\r\n                if (response.status !== 200){\r\n                    throw new Error('Unsuccessful request to deckofcardsapi.com');\r\n                }\r\n                return response.json()\r\n            })\r\n            .then(json => dispatch(fetchDeckSuccess(json)))\r\n            .catch(error => dispatch(fetchDeckError(error)));\r\n}\r\n\r\nexport const fetchDrawCard = deck_id => dispatch => {\r\n        return fetch (`${API_ADDRESS}/deck/${deck_id}/draw`)\r\n            .then(response => {\r\n                if (response.status !== 200){\r\n                    throw new Error('Unsuccessful request to deckofcardsapi.com');\r\n                }\r\n                return response.json()\r\n            })\r\n            .then(json => {\r\n                dispatch({\r\n                type: DECK_DRAW.FETCH_SUCCESS,\r\n                cards: json.cards,\r\n                remaining: json.remaining\r\n            })\r\n        })\r\n        .catch(error => dispatch({ type: DECK_DRAW.FETCH_ERROR, message: error.message}));\r\n}"},"lineCount":null}},"error":null,"hash":"be4320f6ff86df5eddd8df37faef9c5a","cacheData":{"env":{}}}
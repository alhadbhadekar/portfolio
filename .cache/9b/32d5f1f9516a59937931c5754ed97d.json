mponentConnector","generated":{"line":56,"column":15},"source":"projects/evens-or-odds/components/Instructions.js","original":{"line":52,"column":15}},{"generated":{"line":56,"column":33},"source":"projects/evens-or-odds/components/Instructions.js","original":{"line":52,"column":33}},{"name":"Instructions","generated":{"line":56,"column":34},"source":"projects/evens-or-odds/components/Instructions.js","original":{"line":52,"column":34}},{"generated":{"line":56,"column":46},"source":"projects/evens-or-odds/components/Instructions.js","original":{"line":52,"column":33}},{"generated":{"line":56,"column":49},"source":"projects/evens-or-odds/components/Instructions.js","original":{"line":54,"column":0}},{"generated":{"line":57,"column":0},"source":"projects/evens-or-odds/components/Instructions.js","original":{"line":55,"column":0}},{"generated":{"line":58,"column":0},"source":"projects/evens-or-odds/components/Instructions.js","original":{"line":58,"column":0}},{"generated":{"line":59,"column":0},"source":"projects/evens-or-odds/components/Instructions.js","original":{"line":59,"column":0}},{"generated":{"line":60,"column":0},"source":"projects/evens-or-odds/components/Instructions.js","original":{"line":60,"column":0}},{"generated":{"line":61,"column":0},"source":"projects/evens-or-odds/components/Instructions.js","original":{"line":61,"column":0}},{"generated":{"line":62,"column":0},"source":"projects/evens-or-odds/components/Instructions.js","original":{"line":62,"column":0}}],"sources":{"projects/evens-or-odds/components/Instructions.js":"import React from 'react';\r\nimport { connect } from 'react-redux';\r\nimport { expandInstructions, collapseInstructions } from '../actions/settings';\r\n\r\nconst Instructions = props => {\r\n    //console.log('props', props);\r\n    const { instructionsExpanded, expandInstructions, collapseInstructions } = props;\r\n    \r\n    //Return when instructions expanded is true\r\n    if (instructionsExpanded) {\r\n        return (\r\n            <div>\r\n                <h3>Instructions</h3>\r\n                <p>Welcome to evens or odds. The game goes like this</p>\r\n                <p>The deck is shuffled. Then choose: will the next card be even or odd</p>\r\n                <p>Make a choice on every draw, and see how many you get right!</p>\r\n                <p>(Face cards don't count)</p>\r\n                <br />\r\n                <button onClick={collapseInstructions}>Show less</button>\r\n            </div>\r\n        );\r\n    }\r\n    //Return when instructions expanded is false\r\n\r\n    return (\r\n        <div>\r\n            <h3>Instructions</h3>\r\n            <p>Welcome to evens or odds. The game goes like this...</p>\r\n            <br />\r\n            <button onClick={expandInstructions}>Read more</button>\r\n        </div>\r\n    )\r\n}\r\n\r\n//It baiscally defineds what parts of redux store we want to grab\r\nconst mapStateToProps = state => {\r\n    return { instructionsExpanded: state.settings.instructionsExpanded };\r\n}\r\n\r\nconst mapDispatchToProps = dispatch => {\r\n    //return keys whose methods are the ones we want to bind to component's this object\r\n    return{\r\n        expandInstructions: () => dispatch(expandInstructions()),\r\n        collapseInstructions: () => dispatch(collapseInstructions())\r\n    }\r\n}\r\n\r\n//This fucntion also attaches properties we return from mapStateToProps and attach it to props object of component\r\n\r\n\r\nconst componentConnector = connect(mapStateToProps, mapDispatchToProps);\r\nexport default componentConnector(Instructions);\r\n\r\n//Above 2 lines can be shortenend to one line as below\r\n//export default connect(mapStateToProps, mapDispatchToProps)(Instructions);\r\n\r\n\r\n//Another way of writing all above code\r\n//export default connect(\r\n//    state => ({ instructionsExpanded: state.instructionsExpanded }), \r\n//    { expandInstructions, collapseInstructions }\r\n//    )(Instructions);"},"lineCount":null}},